10 CUR0:CLS
30 FOR X=0 TO 200
40 FOR Y=0 TO 100
50 I=0
60 CX=(X-100)/50
70 CY=(Y-100)/50
80 VX=0
90 VY=0
100 REM START CALCULATION
110 I=I+1
120 XX=VX*VX
130 YY=VY*VY
140 VY=CY+(VX+VX)*VY
150 VX=CX+XX-YY
160 IF I<63 AND (XX+YY)<4 THEN GOTO 100
170 YR=200-Y
180 PSET X,Y,I
190 PSET X,YR
200 NEXT Y
210 NEXT X
300 CUR1
