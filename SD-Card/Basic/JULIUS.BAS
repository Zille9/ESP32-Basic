10 CLS
20 A=-1.5
30 B=-0.95
40 C=-0.798
50 D=0.1618
60 E=80
70 F=240/320
80 G=2.80
90 H=G/320
95 I=(G*F)/240
100 L=TIMER
110 FOR X=0 TO 319
120 CX=X*H+A
130 FOR Y=0 TO 239
140 CY=Y*I+B
150 ZR=CX
160 ZI=CY
170 N=0
200 J=(ZR^2)-(ZI^2)+C
210 NI=2*ZR*ZI+D
220 ZR=J
230 ZI=NI
240 N=N+1
280 IF (ZR^2)-(ZI^2)<4 AND N<=E THEN GOTO 200
300 M=N
302 P=N*2
304 O=N*4
305 IF M>255 THEN M=M&255
306 IF P>255 THEN P=P&255
307 IF O>255 THEN O=O&255
310 N=(O*P*M)&63
320 PSET X,Y,N
330 NEXT Y
340 NEXT X
345 PEN 62
350 PRINT (TIMER-L)/1000/60;" Minuten";
360 GOTO 360
