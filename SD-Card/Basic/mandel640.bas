100 CLS: CUR 0
110 Z=32
120 FOR D=0 TO 100
130 FOR C=100 TO 330
140 A=0:B=0:N=0
150 IF N<Z THEN GOTO 300
160 IF N=Z THEN N=63
170 IF N>63 THEN N=N-63
175 COLOR N,1
180 LINE 3*C-340,100-D,3*C-338,100-D
190 LINE 3*C-340,100+D,3*C-338,100+D
220 NEXT C
230 NEXT D
240 CUR1
250 END
300 X=A*A: Y=B*B
310 P=X+Y
350 IF P<20000 THEN GOTO 370
360 GOTO 160
370 F=(A*A-B*B)/50-220+C
380 B=A*B/25+D
390 A=F: N=N+1
400 GOTO 150
