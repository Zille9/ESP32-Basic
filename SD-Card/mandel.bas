2000 CLS
2010 T=TIMER
2030 L=64
2040 FOR I=0 TO 319
2050 FOR J=0 TO 239
2060 U=I/120-1.7
2070 V=J/120-1
2080 X=U
2090 Y=V
2100 N=0
2120 R=X*X
2130 Q=Y*Y
2140 IF R+Q>4 THEN GOTO 2200
2145 IF N>=L THEN GOTO 2200
2150 Y=2*X*Y+V
2160 X=R-Q+U
2170 N=N+1
2180 GOTO 2120
2200 IF N<10 THEN C=4
2205 ELSE C=N
2210 PSET I,J,C|N
2220 NEXT J
2230 NEXT I
2235 G=TIMER-T
2240 PRINT"Zeit:";G/1000/60;" Minuten"
2250 END
